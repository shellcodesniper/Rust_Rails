<% content_for(:side_bar) do %>
<%= render 'shared/navbar', :current => "notice" %>
<% end %>
<% content_for(:footer) do %>
<%= render 'shared/footer' %>
<% end %>

<div class="container">
	<p>
	<strong>Title:</strong>
	<%= @notice.title %>
	</p>

	<p>
	<strong>Content:</strong>
	<%= @notice.content %>
	</p>

	<%= link_to 'Edit', edit_notice_path(@notice) %> |
	<%= link_to 'Back', notices_path %>

</div>
